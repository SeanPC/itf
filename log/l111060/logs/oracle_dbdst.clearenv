group oradb_grp (
	SystemList = { l111060 = 0, l111061 = 1 }
	Parallel = 1
	AutoStartList = { l111060, l111061 }
	)

	CFSMount archivemnt (
		MountPoint = "/archive"
		BlockDevice = "/dev/vx/dsk/oradg9318_dbarch/archive"
		)

	CFSMount dbmnt (
		MountPoint = "/dbdata"
		BlockDevice = "/dev/vx/dsk/oradg9318_dbarch/dbdata"
		)

	Oracle oradb (
		Sid @l111060 = orcl2
